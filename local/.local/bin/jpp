#!/usr/bin/env python3

import json
import re
import sys

data = sys.stdin.read()
object = json.loads(data)

if (prefix := re.search(r"^[ \t]*", data).group(0)) == "":
    json.dump(object, sys.stdout, indent=2)
else:
    data = json.dumps(object, indent=2)
    sys.stdout.write(prefix + data.replace("\n", "\n" + prefix))
